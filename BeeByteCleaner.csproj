<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net471</TargetFramework>
        <ImplicitUsings>disable</ImplicitUsings>
        <StartupObject>BeeByteCleaner.CLI.Program</StartupObject>
        <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Mono.Cecil" Version="0.11.6" />
    </ItemGroup>

    <ItemGroup>
      <Compile Include="src\BeeByteCleaner.CLI\**\*.cs" />
      <Compile Include="src\BeeByteCleaner.Core\**\*.cs" />
      <Compile Include="src\BeeByteCleaner.Runtime\**\*.cs" />
    </ItemGroup>

</Project>
